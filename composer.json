{
    "name": "bentools/doctrine-native-enums",
    "description": "Provides 1st class suport for native PHP enums in Doctrine.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BenTools\\Doctrine\\NativeEnums\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BenTools\\Doctrine\\NativeEnums\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Beno!t POLASZEK",
            "email": "bpolaszek@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "^2.13 >=2.13.5 || ~3.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.18",
        "symfony/http-kernel": "~5.0",
        "doctrine/doctrine-bundle": "^2.4",
        "symfony/framework-bundle": "^5.3",
        "symfony/yaml": "^5.3",
        "doctrine/orm": "^2.9",
        "symfony/validator": "^5.3",
        "ext-sqlite3": "*",
        "squizlabs/php_codesniffer": "^3.6",
        "mockery/mockery": "^1.4"
    }
}
